class MathOperations {
    int add(int a, int b){
    return a + b; 
}
    double add(double a, double b) { 
return a + b; 
}
}

public class Main {
    public static void main(String[] args) {
        MathOperations mo = new MathOperations();
        System.out.println(mo.add(2, 3));
        System.out.println(mo.add(2.5, 3.5));
    }
}
