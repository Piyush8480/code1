class Animal {
    void eat() { System.out.println("Eating"); }
}

class Mammal extends Animal {
    void walk() { System.out.println("Walking"); }
}

class Human extends Mammal {
    void talk() { System.out.println("Talking"); }
}

public class Main {
    public static void main(String[] args) {
        Human h = new Human();
        h.eat();
        h.walk();
        h.talk();
    }
}
