class Animal {
    void sleep() { System.out.println("Sleeping..."); }
}

class Cat extends Animal {
    void meow() { System.out.println("Meowing..."); }
}

class Dog extends Animal {
    void bark() { 
System.out.println("Barking..."); 
}
}

public class Main {
    public static void main(String[] args) {
        Cat c = new Cat();
        Dog d = new Dog();
        c.sleep();
        c.meow();
        d.sleep();
        d.bark();
    }
}
